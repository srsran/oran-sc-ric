		ueType-Choice-Group				EventTrigger-UE-Info-Item-Choice-Group,
		...,
		ueQuery							UEQuery
	},
	logicalOR							LogicalOR 							OPTIONAL,
	...
}

UEQuery ::= SEQUENCE {
	partialUEID							PartialUEID,
	ranParameterTesting					RANParameter-Testing				OPTIONAL,
	...
}

